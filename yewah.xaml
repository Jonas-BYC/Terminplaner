<Window x:Class="Terminplaner.yewah"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Terminplaner"
      mc:Ignorable="d"
      Title="yewah" Height="450" Width="800">
    <Grid>
        
        <StackPanel>
            <TextBlock Text="Do you want to continue?" Margin="0,0,0,10" FontSize="14" TextAlignment="Center"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Content="OK" Width="75" Margin="5" Click="Ok_Click"/>
                <Button Content="Cancel" Width="75" Margin="5" Click="Cancel_Click"/>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
